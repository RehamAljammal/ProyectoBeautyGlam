@{
    ViewBag.Title = "Repetir Diagnóstico";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="section-space sk-page">
    <div class="container sk-container">

        <div class="sk-card text-center sk-retry">

            <span class="sk-badge">Ajuste de rutina</span>

            <h3 class="sk-retry-title">¿Quieres ajustar tu rutina?</h3>
            <p class="sk-retry-text">
                Puedes repetir el diagnóstico en cualquier momento para obtener una rutina más precisa ✨
            </p>

            <div class="sk-action">
                <a href="@Url.Action("Cuestionario","AsistenteSkincare")"
                   class="btn sk-btn">
                    Volver a empezar ♡
                </a>
            </div>

        </div>

    </div>
</section>
